{
  "name": "altheia-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --turbopack",
    "postinstall": "prisma generate",
    "build": "next build",
    "build:prod": "cp prisma/schema.production.prisma prisma/schema.prisma && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force",
    "db:push:prod": "cp prisma/schema.production.prisma prisma/schema.prisma && prisma db push",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:install": "playwright install chromium"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.12.0",
    "@prisma/client": "^6.4.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-toast": "^1.2.6",
    "@types/nodemailer": "^7.0.11",
    "@vercel/blob": "^2.3.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.475.0",
    "next": "^16.0.0",
    "next-themes": "^0.4.4",
    "nodemailer": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-qr-code": "^2.0.18",
    "stripe": "^17.7.0",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "prisma": "^6.4.0",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
